{
  "name": "react-package",
  "scripts": {
    "start": "parcel index.html",
    "serve": "http-server dist -p 3000",
    "build": "npm run build:app && cp _redirects dist/",
    "build:app": "parcel build index.html --public-url ./ --no-scope-hoist",
    "build:staging": "env-cmd -f .env.staging npm run build:app && cp _redirects dist/",
    "serve:staging": "env-cmd -f .env.staging npm run serve",
    "prettier:check": "npx prettier --check .",
    "prettier:format": "npx prettier --write ."
  },
  "dependencies": {
    "@boringer-avatars/react": "^0.2.1",
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@lottiefiles/lottie-player": "^1.7.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "^5.12.2",
    "@mui/styled-engine-sc": "^5.12.0",
    "@mui/x-date-pickers": "^6.2.1",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "html2pdf.js": "^0.10.1",
    "http-server": "^14.1.1",
    "jspdf": "2.5.1",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "query-string": "^8.1.0",
    "quill": "^1.3.7",
    "quill-comment": "^1.3.0",
    "quill-delta-to-html": "^0.12.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-quill": "^2.0.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "5.2.0",
    "react-scroll-into-view": "^1.12.1",
    "styled-components": "^5.3.10",
    "uuid": "^9.0.0",
    "env-cmd": "^10.1.0",
    "parcel": "2.8.1",
    "parcel-reporter-static-files-copy": "^1.3.4"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.1",
    "browserslist": "4.19.3",
    "buffer": "^5.7.1",
    "cross-env": "^7.0.3",
    "prettier": "2.8.4",
    "process": "^0.11.10"
  }
}
